{
  "name": "taxes-payment-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "types:check": "tsc --noEmit --incremental",
    "prisma": "dotenv -e .env.development -- npx prisma",
    "prisma:push": "dotenv -e .env.development -- npx prisma db push",
    "prisma:migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:generate": "dotenv -e .env.development -- npx prisma generate",
    "seed:admin": "dotenv -e .env.development -e .env.local -- tsx --tsconfig tsconfig.seed.json prisma/seed-admin.ts",
    "seed:dev": "dotenv -e .env.development -- tsx --tsconfig tsconfig.seed.json prisma/seed-dev.ts"
  },
  "dependencies": {
    "@azure/identity": "^4.6.0",
    "@azure/msal-browser": "^4.0.2",
    "@azure/msal-node": "^3.1.0",
    "@hookform/resolvers": "^3.10.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/client": "^6.2.1",
    "@tailwindcss/postcss": "^4.0.0",
    "@tanstack/react-table": "^8.20.6",
    "euplatesc": "^1.0.5",
    "lodash": "^4.17.21",
    "next": "15.1.6",
    "next-auth": "^4.24.11",
    "next-intl": "^3.26.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.0.1",
    "react-select": "^5.9.0",
    "yup": "^1.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.10",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@types/react-google-recaptcha": "^2.1.9",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^8",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prisma": "^6.2.1",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "overrides": {
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3"
  }
}
