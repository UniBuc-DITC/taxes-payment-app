{
  "name": "taxes-payment-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "types:check": "tsc --noEmit --incremental",
    "prisma": "dotenv -e .env.development -- npx prisma",
    "prisma:push": "dotenv -e .env.development -- npx prisma db push",
    "prisma:migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:generate": "dotenv -e .env.development -- npx prisma generate",
    "seed:admin": "dotenv -e .env.development -e .env.local -- tsx --tsconfig tsconfig.seed.json prisma/seed-admin.ts",
    "seed:dev": "dotenv -e .env.development -- tsx --tsconfig tsconfig.seed.json prisma/seed-dev.ts"
  },
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@azure/msal-browser": "^4.26.1",
    "@azure/msal-node": "^3.8.1",
    "@hookform/resolvers": "^5.2.2",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/client": "^6.19.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-table": "^8.21.3",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "next": "16.0.1",
    "next-auth": "^4.24.13",
    "next-intl": "^4.5.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.66.0",
    "react-icons": "^5.5.0",
    "react-select": "^5.10.2",
    "yup": "^1.7.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/react-google-recaptcha": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^11",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
