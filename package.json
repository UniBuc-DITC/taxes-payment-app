{
  "name": "taxes-payment-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "types:check": "tsc --noEmit --incremental",
    "prisma": "dotenv -e .env.development -- npx prisma",
    "prisma:push": "dotenv -e .env.development -- npx prisma db push",
    "prisma:migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:generate": "dotenv -e .env.development -- npx prisma generate",
    "seed:admin": "dotenv -e .env.development -e .env.local -- tsx --tsconfig tsconfig.seed.json prisma/seed-admin.ts",
    "seed:dev": "dotenv -e .env.development -- tsx --tsconfig tsconfig.seed.json prisma/seed-dev.ts"
  },
  "dependencies": {
    "@azure/identity": "^4.10.0",
    "@azure/msal-browser": "^4.13.0",
    "@azure/msal-node": "^3.6.0",
    "@hookform/resolvers": "^3.10.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/client": "^6.8.2",
    "@tailwindcss/postcss": "^4.1.8",
    "@tanstack/react-table": "^8.21.3",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "next": "^15.3.3",
    "next-auth": "^4.24.11",
    "next-intl": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.57.0",
    "react-icons": "^5.5.0",
    "react-select": "^5.10.1",
    "yup": "^1.6.1",
    "zod": "^3.25.49"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/react-google-recaptcha": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.1.6",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prisma": "^6.8.2",
    "tailwindcss": "^4.1.8",
    "tsx": "^4.19.4",
    "typescript": "^5.7.3"
  }
}
